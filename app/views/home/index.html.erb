<h1>Home#index</h1>
<p>Find me in app/views/home/index.html.erb</p>

<% if params[:search] %>
    <div>
        <%= response %>
        <%# data %>
    </div>
<% end %>

<%= form_with url: home_index_url, method: :get do |form| %>
    <%= form.label :search, "Search for shows:" %>
    <%= form.text_field :search %>
    <%= form.submit "Search" %>
<% end %>